<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu">

    <Button
        android:id="@+id/PartesHoras"
        android:layout_width="167dp"
        android:layout_height="58dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="219dp"
        android:layout_marginBottom="569dp"
        android:background="@drawable/botones_redondos"
        android:text="@string/PartesHoras"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6" />

    <Button
        android:id="@+id/Fichajes"
        android:layout_width="166dp"
        android:layout_height="60dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="565dp"
        android:background="@drawable/botones_redondos"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toEndOf="@+id/PartesHoras"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/Proyectos"
        android:layout_width="157dp"
        android:layout_height="70dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="218dp"
        android:layout_marginBottom="390dp"
        android:background="@drawable/botones_redondos"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PartesHoras" />

    <Button
        android:id="@+id/Perfil"
        android:layout_width="152dp"
        android:layout_height="71dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="390dp"
        android:background="@drawable/botones_redondos"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Proyectos"
        app:layout_constraintTop_toBottomOf="@+id/Fichajes" />

    <Button
        android:id="@+id/Salir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="234dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>